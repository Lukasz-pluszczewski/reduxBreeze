    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "babelrc-rollup": "3.0.0",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "eslint": "4.5.0",
    "eslint-config-brainhub": "1.6.3",
    "eslint-watch": "3.1.2",
    "mocha": "3.5.0",

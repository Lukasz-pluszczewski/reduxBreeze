  },
  "main": "lib/reduxBreeze.js",
  "module": "lib/reduxBreeze.mjs",
  "jsnext:main": "lib/reduxBreeze.mjs",
  "scripts": {
    "prepublish": "NODE_PATH=src npm run build",
    "prebuild": "rimraf ./lib",
    "build": "NODE_PATH=src rollup -c",
    "watch": "NODE_PATH=src rollup -c -w",
    "lint": "esw rollup.config.js src --color",
    "lint:fix": "esw rollup.config.js src --color --fix",
    "test": "npm run mochatest --silent",
    "test:watch": "npm run mochatest -- --watch",
    "mochatest": "NODE_PATH=src nyc --reporter=html mocha tools/testSetup.js \"./test/**/*.js\""
